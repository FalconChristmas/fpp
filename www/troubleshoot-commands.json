                        "all"
                    ]
                },
                "NTPPeers": {
                    "title": "NTP Peers",
                    "description": "",
                    "cmd": "pgrep ntpd > /dev/null && ntpq -c peers",
                    "platforms": [
                        "all"
                    ]
                },
                "RTC": {
                    "title": "RTC",
                    "description": "",
                    "cmd": "hwclock -r -f [[rtcDevice]]",
                    "platforms": [
                        "all"
                    ]
                }
            }
        },
